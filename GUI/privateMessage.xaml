<Window x:Class="GUI.privateMessage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="privateMessage" Height="150" Width="300">
    <Grid Background="{DynamicResource {x:Static SystemColors.InactiveCaptionTextBrushKey}}">
        <Button x:Name="sendButton" Content="send" HorizontalAlignment="Left" Margin="51,83,0,0" VerticalAlignment="Top" Width="75" Click="sendButton_Click"/>
        <Button x:Name="closeButton" Content="close" HorizontalAlignment="Left" Margin="144,83,0,0" VerticalAlignment="Top" Width="75" Click="closeButton_Click"/>
        <TextBox x:Name="messageBox" HorizontalAlignment="Left" Height="33" Margin="10,35,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="272"/>
        <Label x:Name="label"  HorizontalAlignment="Left" Margin="10,6,0,0" VerticalAlignment="Top" Width="272" Foreground="#FFEE7841"/>
    </Grid>
</Window>
