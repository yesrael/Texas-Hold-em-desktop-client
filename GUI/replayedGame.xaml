<Window x:Class="GUI.replayedGame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="replayedGame" Height="500" Width="750">
    <Grid>
    <Grid.Background>
            <ImageBrush ImageSource="pictures/poker_table.jpg"/>
        </Grid.Background>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="242" Margin="138,133,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="461" Background="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
        <Button x:Name="return_button" Content="Return" HorizontalAlignment="Left" Margin="632,19,0,0" VerticalAlignment="Top" Width="88" Height="25" Click="return_button_Click"/>
    </Grid>
</Window>
